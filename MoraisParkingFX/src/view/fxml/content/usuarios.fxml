                  <TextField fx:id="emailFieldAdd" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="375.0" styleClass="content-field" />
                  <PasswordField fx:id="passwordFieldAdd" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="225.0" styleClass="content-field" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="12.0" layoutY="352.0" spacing="40.0">
               <children>
                  <Button fx:id="buttonAdd" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#createUser" prefHeight="30.0" prefWidth="150.0" styleClass="content-btn" text="Cadastrar">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
