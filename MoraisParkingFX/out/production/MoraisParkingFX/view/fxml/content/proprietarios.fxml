<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.content.ProprietariosController">

    <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
              <Label text="Cadastrar proprietário">
                  <font>
                      <Font size="20.0" />
                  </font>
              </Label>
            <HBox alignment="CENTER" spacing="40.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Nome">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField minWidth="-Infinity" prefWidth="500.0">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </TextField>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Matricula">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <TextField minWidth="-Infinity" prefWidth="150.0">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox layoutX="10.0" layoutY="35.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Nome" />
                        <TextField minWidth="-Infinity" prefWidth="400.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
         <children>
            <Label text="Remover proprietário">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
      </VBox>
    </children>
</VBox>
